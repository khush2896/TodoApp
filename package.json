{
  "name": "gateway",
  "version": "1.0.0",
  "description": "gateway",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon ./server/server.js --exec babel-node",
    "build": "babel server -d build",
    "clean": "rm -rf build",
    "rebuild": "npm run clean && npm run build ",
    "prod": "NODE_ENV=prod node ./build/server.js",
    "forever:start": "NODE_ENV=prod forever start -a ./build/server.js",
    "test": "mocha server/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'"
  },
  "engines": {
    "node": ">=v8.4.0"
  },
  "keywords": [],
  "author": "Asif",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "forever": "^0.15.3",
    "helmet": "^3.8.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.4",
    "logger": "0.0.1",
    "mongoose": "^4.11.9",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-plugin-node": "^5.1.1",
    "expect": "^1.20.2",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0"
  }
}
